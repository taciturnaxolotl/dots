# bore tunnel configuration
# Save this file as "bore.toml" in your project directory

[myapp]
port = 8000

[api]
port = 3000
labels = ["dev", "api"]

[frontend]
port = 5173
labels = ["local"]

[admin]
port = 3001
auth = true
labels = ["admin"]

[database]
port = 5432
protocol = "tcp"
labels = ["postgres"]

[game-server]
port = 27015
protocol = "udp"
labels = ["game"]
